
{% extends 'base.html.twig' %}

{% block body %}
    <div id="form">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <h1>Редактировать заказ-наряд</h1>
                </div>
                <div class="col-md-6">
                    {{ form_start(edit_form) }}
                    <div class="formItem">{% if edit_form.dateOpen %}{{ form_row(edit_form.dateOpen) }}{% endif %}</div>
                    <div class="formItem">{{ form_row(edit_form.workeropen) }}</div>
                    <div class="formItem">{% if edit_form.dateClose %}{{ form_row(edit_form.dateClose) }}{% endif %}</div>
                    <div class="formItem">{% if edit_form.datePayment %}{{ form_row(edit_form.datePayment) }}{% endif %}</div>
                    <div class="formItem">{{ form_row(edit_form.workerclose) }}</div>
                    <div class="formItem">{{ form_row(edit_form.name) }}</div>
                    <div class="formItem">{{ form_row(edit_form.brandId) }}</div>
                    <div class="formItem">{{ form_row(edit_form.carId) }}</div>
                    <div class="formItem">{% if edit_form.bodyId %}{{ form_row(edit_form.bodyId) }}{% endif %}</div>
                    <div class="formItem">{{ form_row(edit_form.color) }}</div>
                    <div class="formItem">{{ form_row(edit_form.number) }}</div>
                    <div class="formItem">{{ form_row(edit_form.vin) }}</div>
                    <div class="formItem">{{ form_row(edit_form.description) }}</div>
                    <div class="formItem">{{ form_row(edit_form.damages) }}</div>
                    <div class="formItem">{{ form_row(edit_form.phone) }}</div>
                    <div class="formItem">{{ form_row(edit_form.email) }}</div>
                    <div class="formItem">{{ form_row(edit_form.payment) }}</div>
                    <div class="formItem">{{ form_row(edit_form.price) }}</div>
                    <div class="formItem">
                        <ul style="list-style-type: none" id="workerorders-fields-list"
                            data-prototype="{{ form_widget(edit_form.workerorders.vars.prototype)|e('html_attr') }}"
                            data-widget-tags="{{ '<li></li>'|e }}">
                            {% for workerordersField in edit_form.workerorders %}
                                <li>
                                    {{ form_errors(workerordersField) }}
                                    {{ form_widget(workerordersField) }}
                                    <a href="#" class="remove-tag btn btn-danger">Удалить</a>
                                </li>
                            {% endfor %}
                        </ul>

                        <div><button type="button" class="add-another-collection-widget btn btn-info" data-list="#workerorders-fields-list">Добавить сотрудника и услугу</button></div>
                    </div>
                    {% if not order.close %}
                        <div class="formItem">{{ form_row(edit_form.close) }}</div>
                    {% else %}
                        <div class="formItem" style="display: none;">{{ form_row(edit_form.close, {'attr': {'checked': 'checked'}}) }}</div>
                    {% endif %}
                    <div class="formItem">
                        <a data-toggl="2" style="margin-top: 20px; display: block;" href='#'>Распечатать Наряд на работу</a>
                    </div>
                    <div class="formItem">
                        <a data-toggl="1" style="margin-top: 20px; display: block;" href='#'>Распечатать Заказ-наряд</a>
                    </div>
                    <div class="formItem form-button">
                        <ul>
                            <li>
                                {% if not order.close %}
                                    <input style="margin-top: 20px;" class="btn btn-success" type="submit" value="Сохранить" />
                                {% else %}
                                    <input style="margin-top: 20px;" class="btn btn-success" type="submit" value="Сохранить" disabled />
                                {% endif %}
                                {{ form_end(edit_form) }}
                            </li>
                            <li>{{ form_start(delete_form) }}
                                <input style="margin-top: 20px;" class="btn btn-danger" type="submit" value="Удалить">
                                {{ form_end(delete_form) }}
                            </li>
                            <li>
                                <a style="margin-top: 20px;" class="btn btn-primary" href="{{ path('orders_index') }}">Назад</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block ordersform %}{% endblock %}
{% block print %}
    <div class="workorder">
        <table>
            <thead>
                <tr>
                    <td>
                        <p>ИП Манков Вячеслав Вячеславович</p>
                        <p>Телефон: +79268546655</p>
                        <p>Email: info@good-look.moscow</p>
                        <p>Сайт: www.good-look.moscow</p>
